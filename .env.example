REM LLM 엔진 설정
set VLLM_ENABLED=true
set VLLM_BASE_URL=http://localhost:8000
set SGLANG_ENABLED=true
set SGLANG_BASE_URL=http://localhost:30000

REM Oracle 데이터베이스 설정 (선택사항)
set DB_HOST=localhost
set DB_PORT=1521
set DB_SID=XE
set DB_USERNAME=llmchat
set DB_PASSWORD=password

REM Spring 프로필
set SPRING_PROFILES_ACTIVE=dev

REM JVM 옵션 (Windows)
set JAVA_OPTS=-Xms512m -Xmx2g -XX:+UseG1GC -Dfile.encoding=UTF-8